{
    "name": "trogers1884/extended-migration",
    "description": "Extended migration support for Laravel with multi-schema PostgreSQL support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "trogers1884"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Trogers1884\\ExtendedMigration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trogers1884\\ExtendedMigration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trogers1884\\ExtendedMigration\\ExtendedMigrationServiceProvider"
            ],
            "aliases": {
                "SchemaManager": "Trogers1884\\ExtendedMigration\\Facades\\SchemaManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
