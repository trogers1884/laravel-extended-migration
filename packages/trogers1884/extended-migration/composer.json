{
    "name": "trogers1884/laravel-extended-migration",
    "description": "Laravel package for managing multiple PostgreSQL schema migrations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Rogers",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Trogers1884\\ExtendedMigration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trogers1884\\ExtendedMigration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trogers1884\\ExtendedMigration\\ExtendedMigrationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
